  "info": {
    "description": "This is the Blue Command API\n\nPlease use the login endpoint to obtain a JWT token and use it in subsequent calls via the Authorization: Beaerer JWT header\nscanner RPC endpoints only require Authentication: Bearer password",
    "title": "BlueCommand API",
    "version": "0.0.1"
  },
  "paths": {
    "/api/things": {
      "get": {
        "description": "Gets a list of things",
        "tags": [
          "THINGS"
        ],
        "summary": "Find Things",
        "operationId": "ThingsFind",
        "parameters": [
          {
            "type": "int",
            "description": "Number of records to return",
            "name": "limit",
            "in": "query"
          },
          {
            "type": "int",
            "description": "Offset of records to return",
            "name": "offset",
            "in": "query"
          },
          {
            "type": "string",
            "description": "Filter id",
            "name": "id",
            "in": "query"
          },
          {
            "type": "string",
            "description": "Filter name",
            "name": "name",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Thing Objects",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/gorestapi_Thing"
              }
            }
          }
        }
      },
      "post": {
        "description": "Creates or saves a thing. Omit the ID to auto generate.\nPass an existing ID to update.",
        "tags": [
          "THINGS"
        ],
        "summary": "Create/Save Thing",
        "operationId": "ThingSave",
        "parameters": [
          {
            "type": "object",
            "description": "Thing to Save/Update",
            "name": "thing",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/gorestapi_ThingExample"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User Object",
            "schema": {
              "$ref": "#/definitions/gorestapi_Thing"
            }
          }
        }
      }
    },
    "/api/things/{id}": {
      "get": {
        "description": "Fetches a Thing",
        "tags": [
          "THINGS"
        ],
        "summary": "Get a Thing",
        "operationId": "ThingGetByID",
        "parameters": [
          {
            "type": "string",
            "description": "Thing ID to fetch",
            "name": "id",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Thing Object",
            "schema": {
              "$ref": "#/definitions/gorestapi_Thing"
            }
          }
        }
      },
      "delete": {
        "description": "Deletes a Thing",
        "tags": [
          "THINGS"
        ],
        "summary": "Delete a Thing",
        "operationId": "ThingDeleteByID",
        "parameters": [
          {
            "type": "string",
            "description": "Thing ID to delete",
            "name": "id",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          }
        }
      }
    }
  },
